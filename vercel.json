{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "server.js"
    },
    {
      "source": "/api/(.*)", 
      "destination": "https://doctor-appointment-system-beta-one.vercel.app/api/$1" 
    }
  ]
}


